<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Trumphurst Ltd.</Authors>
    <Company>Trumphurst Ltd.Trumphurst Ltd.</Company>
    <Description>C# Api to Mattermost</Description>
    <Copyright>(c) 2019-2020 Trumphurst Ltd.</Copyright>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <PackageTags>Mattermost Api C#</PackageTags>
    <PackageProjectUrl>https://github.com/nikkilocke/MattermostApi</PackageProjectUrl>
    <RepositoryUrl>https://github.com/nikkilocke/MattermostApi</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageReleaseNotes>Made bool? fields in NotifyProps a string instead of bool?, as they are not recognised by the server if set to false.
</PackageReleaseNotes>
    <Version>1.0.35</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MimeMapping" Version="1.0.1.12" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
